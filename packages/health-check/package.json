{
  "name": "health-check",
  "version": "1.0.0",
  "main": "dist/health-check.js",
  "type": "module",
  "exports": {
    ".": "./dist/health-check.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm install",
    "build": "npm run clean && npm run format && tsc",
    "prestart": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "start": "node -r dotenv/config ./dist/index.js",
    "dev": "node -r dotenv/config ./dist/index.js",
    "start:full": "node -r dotenv/config ./dist/index.js --full",
    "start:suggest": "node ./dist/suggest-repos.js",
    "start:health": "node ./dist/health-check.js",
    "start:readme": "node ./dist/generate-readme.js",
    "start:advocate-activity": "node ./dist/cloud-advocate-activity.js",
    "start:contributor-index": "node -r dotenv/config ./dist/index.js --contributor-index",
    "start:health-check": "node -r dotenv/config ./dist/index.js --health-check",
    "start:generate-readme": "node -r dotenv/config ./dist/index.js --generate-readme",
    "start:suggest-repos": "node -r dotenv/config ./dist/index.js --suggest-repos",
    "start:contributor-activity": "node -r dotenv/config ./dist/index.js --contributor-activity",
    "start:workflow-report": "node -r dotenv/config ./dist/index.js --workflow-report",
    "start:infrastructure-report": "node -r dotenv/config ./dist/index.js --infrastructure-report",
    "start:repo-index": "node -r dotenv/config ./dist/index.js --repo-index",
    "start:auth-test": "node -r dotenv/config ./github_auth_test.js",
    "start:scope-test": "node -r dotenv/config ./dist/quick/check-token-scope.js",
    "start:graphql-test": "node -r dotenv/config ./dist/quick/graphql-query.js",
    "test": "vitest run --coverage"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@octokit/core": "^6.1.5",
    "@octokit/rest": "^21.1.1",
    "dotenv": "^16.5.0",
    "octokit": "^4.1.4"
  },
  "devDependencies": {
    "@octokit/plugin-rest-endpoint-methods": "^14.0.0",
    "@octokit/types": "^14.0.0",
    "@tsconfig/recommended": "^1.0.8",
    "@types/node": "^22.15.3",
    "@vitest/coverage-v8": "^3.2.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.2"
  }
}
