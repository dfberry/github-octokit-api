{
  "name": "health-check",
  "version": "1.0.0",
  "main": "dist/health-check.js",
  "type": "module",
  "exports": {
    ".": "./dist/health-check.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:tsc": "tsc",
    "build:tsc:debug": "tsc --pretty --traceResolution --noEmit",
    "build": "npm run clean && npm run format && npm run build:tsc",
    "prestart": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" ",
    "lint:check": "eslint \"src/**/*.ts\"",
    "start": "npm run start:index",
    "start:index": "node -r dotenv/config ./dist/index.js",
    "test": "vitest run --coverage"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@dfb/db": "file:../sqlite-typeorm-db",
    "@dfb/finddb": "file:../finddb",
    "@dfb/octokit": "file:../octokit",
    "dotenv": "^16.5.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.25",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@eslint/json": "^0.12.0",
    "@tsconfig/recommended": "^1.0.8",
    "@types/node": "^22.15.3",
    "eslint": "^9.30.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vitest": "^3.2.2"
  }
}
